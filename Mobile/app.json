{
  "expo": {
    "name": "GLC MyRent",
    "slug": "GLC MyRent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo_main.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#657F31"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#657F31"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/logo_main.png"
    },
    "plugins": [
      "expo-video",
      "expo-font",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#657F31",
          "image": "./assets/logo_main.png",
          "imageWidth": 200
        }
      ]
    ]
  }
}
